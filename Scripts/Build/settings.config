<?xml version="1.0" encoding="utf-8"?>
<project name="CloudInitNETSettings" xmlns="http://nant.sf.net/release/0.85/nant.xsd">
  <!-- Framework Properties -->
  <property overwrite="false" name="nant.settings.currentframework" value="net-4.0" />
  <property overwrite="false" name="framework.dir" value="${framework::get-framework-directory(framework::get-target-framework())}" />

  <!-- Company Properties -->
  <property overwrite="false" name="company.name" value="Brian Wight" />

  <!-- Compile Properties -->
  <property overwrite="false" name="msbuild.configuration"  value="Release" />
  <property overwrite="false" name="msbuild.platform" value="Any CPU" />

  <!-- Assembly Properties -->
  <property overwrite="false" name="version.major" value="1" />
  <property overwrite="false" name="version.minor" value="3" />
  
  <!-- Directory Properties-->
  <property overwrite="false" name="current.dir" value="__NONE__" />
  <property overwrite="false" name="source.dir" value="${current.dir}\Source" />
  <property overwrite="false" name="build.dir" value="${current.dir}\Build" />
  <property overwrite="false" name="tools.dir" value="${current.dir}\Tools" />
  <property overwrite="false" name="release.dir" value="${current.dir}\Release" />
  <property overwrite="false" name="external.dir" value="${current.dir}\External" />
  <property overwrite="false" name="artifacts.dir" value="${current.dir}\Build\Artifacts" />
  <property overwrite="false" name="output.dir" value="${current.dir}\Build\Output" />
  <property overwrite="false" name="scripts.dir" value="${current.dir}\Scripts" />
  <property overwrite="false" name="otherscripts.dir" value="${current.dir}\Scripts\Other" />
  <property overwrite="false" name="buildscripts.dir" value="${current.dir}\Scripts\Build" />
  
  <!-- Absolute Directory Properties -->
  <property overwrite="false" name="source.dir.abs" value="${path::get-full-path(source.dir)}" />
  <property overwrite="false" name="build.dir.abs" value="${path::get-full-path(build.dir)}" />  
</project>